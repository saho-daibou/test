
public class InkuriDekurimento {

	public static void main(String[] args) {
		// 6.加算子、減算子
		System.out.println("前置加算子");
		int a = 10;
		System.out.println(++a);
		System.out.println(++a);
		System.out.println(++a);
		System.out.println(++a);
		
		System.out.println("\r\n後置加算子");
		int b = 10;
		System.out.println(b++);
		System.out.println(b++);
		System.out.println(b++);
		System.out.println(b++);
		
		System.out.println("\r\n前置減算子");
		int c = 10;
		System.out.println(--c);
		System.out.println(--c);
		System.out.println(--c);
		System.out.println(--c);
		
		System.out.println("\r\n後置減算子");
		int d = 10;
		System.out.println(c--);
		System.out.println(c--);
		System.out.println(c--);
		System.out.println(c--);
		

	}

}
