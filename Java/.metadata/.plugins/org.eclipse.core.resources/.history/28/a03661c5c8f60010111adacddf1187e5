
public class Sisokuenzan {

	public static void main(String[] args) {
		// 5.四則演算、剰余
		int a = 3+3;
		System.out.println(a);
		
		int a1 = 4+3;
		System.out.println(a1);
		
		int a2 = -8 -3;
		System.out.println(a2);

		var a3 = -5 * 3;
		System.out.println(a3);
		
		var a4 = 4 / 2;
		System.out.println(a4);
		
		
		

	}

}
