
public class awitch {

	public static void main(String[] args) {
		// 9.switch文
		
		int a = 5;
		switch(a) {
		case 0:
			System.out.println("aは0に等しい");
			break;
		case 1:
			System.out.println("aは1に等しい");
			break;
		default:
			System.out.println("aは0でも1でもない");
		}

		
		String b = "青";
		switch(b) {
		case"赤":
			System.out.println("赤組です");
			break;
		case"白":
			System.out.println("白組です");
			break;
		default:
			System.out.println("エラーです");
		}
	}

}
